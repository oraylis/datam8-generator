# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = LF
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.py]
indent_size = 4
indent_style = space

[*.json]
indent_size = 2
indent_style = space

[.gitmodules]
indent_size = 1
indent_style = tab
